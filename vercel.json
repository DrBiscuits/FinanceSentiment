{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/latest-rating",
      "dest": "/api/latest-rating.js"
    },
    {
      "src": "/api/history-7days",
      "dest": "/api/history.js"
    },
    {
      "src": "/screenshots/(.*)",
      "dest": "/public/screenshots/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
